<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.user_windowController2">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="700.0">
         <children>
            <Label fx:id="user_label" alignment="CENTER" layoutX="246.0" layoutY="14.0" prefHeight="20.0" prefWidth="209.0" text="Welcome Text">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TableView fx:id="Team_Table" layoutX="23.0" layoutY="131.0" prefHeight="199.0" prefWidth="659.0">
              <columns>
                  <TableColumn fx:id="Team_ID" prefWidth="150.0" text="Team Id" />
                  <TableColumn fx:id="Team_Name" prefWidth="162.0" text="Team Name" />
                  <TableColumn fx:id="Team_coach" minWidth="0.0" prefWidth="177.0" text="Team Coach" />
                  <TableColumn fx:id="Team_owner" prefWidth="169.0" text="Team Owner" />
              </columns>
            </TableView>
            <Label fx:id="user_text" alignment="CENTER" layoutX="9.0" layoutY="70.0" prefHeight="35.0" prefWidth="685.0" text="Following are the Teams playing in the league administered by you.">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <TextField fx:id="enter_teamid" layoutX="14.0" layoutY="369.0" promptText="Enter Team ID" />
            <TextField fx:id="enter_teamname" layoutX="189.0" layoutY="369.0" promptText="Enter Team Name" />
            <TextField fx:id="enter_teamcoach" layoutX="365.0" layoutY="369.0" promptText="Enter Team Coach" />
            <TextField fx:id="enter_teamowner" layoutX="537.0" layoutY="369.0" promptText="Enter Team Owner" />
            <Button fx:id="add_button" layoutX="154.0" layoutY="477.0" mnemonicParsing="false" onAction="#insertTeam" text="Add Team" />
            <Button fx:id="delete_button" layoutX="514.0" layoutY="477.0" mnemonicParsing="false" onAction="#DeleteTeam" text="Delete Team" />
            <Button fx:id="search_button" layoutX="340.0" layoutY="477.0" mnemonicParsing="false" onAction="#searchLeague" text="Search" />
            <Button fx:id="team_mainmenu" layoutX="327.0" layoutY="545.0" mnemonicParsing="false" onAction="#backtomenu" text="Main Menu" />
            <TextField fx:id="league_id" layoutX="279.0" layoutY="414.0" promptText="Enter League ID" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
